<p id="notice"><%= notice %></p>

<h1>Data Indicators</h1>

<table>
  <thead>
    <tr>
      <th>Federal entity</th>
      <th>Main category</th>
      <th>Category</th>
      <th>Subcategory</th>
      <th>Indicator</th>
      <th>Year</th>
      <th>Unit of measurement</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @data_indicators.each do |data_indicator| %>
      <tr>
        <td><%= data_indicator.federal_entity %></td>
        <td><%= data_indicator.main_category %></td>
        <td><%= data_indicator.category %></td>
        <td><%= data_indicator.subcategory %></td>
        <td><%= data_indicator.indicator %></td>
        <td><%= data_indicator.year %></td>
        <td><%= data_indicator.unit_of_measurement %></td>
        <td><%= link_to 'Show', data_indicator %></td>
        <td><%= link_to 'Edit', edit_data_indicator_path(data_indicator) %></td>
        <td><%= link_to 'Destroy', data_indicator, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Data Indicator', new_data_indicator_path %>
